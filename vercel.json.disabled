{
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "installCommand": "corepack enable && corepack prepare pnpm@10.10.0 --activate && pnpm install",
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node"
        }
    ]
}